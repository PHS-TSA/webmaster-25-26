{
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "biomejs.biome",
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "css.customData": [".vscode/tailwind.json"],
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit"
  }
}
